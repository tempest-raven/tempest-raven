KrinLang.GERMAN.NAVTITLE[0] = "Informationen";
KrinLang.GERMAN.NAVTEXT[0] = "Dein Spiel wurde gespeichert!";
KrinLang.GERMAN.NAVTITLE[1] = "Gordon \'Schnelle-Hand\' Smith";
KrinLang.GERMAN.NAVTEXT[1] = "Psst! Kumpel, komm mal her. Ich hab was du suchst. Beste QualitÃ¤t! Keine Angst, komm nÃ¤her. Ich tuh dir nichts.";
KrinLang.GERMAN.NAVTITLE[2] = "Blutige Spuren";
KrinLang.GERMAN.NAVTEXT[2] = "Dies ist kein normales GefÃ¤ngnis, es gibt Ã¼berall Blutspuren. Diese Spur ist noch neu, und sie riecht komisch. Auf keinen Fall etwas natÃ¼rliches...";
KrinLang.GERMAN.NAVTITLE[3] = "Das Auge von Alcatraz";
KrinLang.GERMAN.NAVTEXT[3] = "Friedrich Nietzsche sagte: \'Wenn du lange in einen Abgrund blickst, blickt der Abgrund auch in dich hinein.";
KrinLang.GERMAN.NAVTITLE[4] = "Wachturm";
KrinLang.GERMAN.NAVTEXT[4] = "Die Lichter suchen uns. Sie wissen dass wir hier sind...";
KrinLang.GERMAN.NAVTITLE[5] = "RebellenstÃ¼tzpunkt";
KrinLang.GERMAN.NAVTEXT[5] = "Was machst du hier? Verlasse diesen Ort, du hast hier nichts verloren. Dieses Dorf ist mit Zombies und Fanatikern infestiert. Wir versuchen nicht aufzufallen, bit gehe bevor jemand uns bemerkt.";
KrinLang.GERMAN.NAVTITLE[6] = "Der Brunnen";
KrinLang.GERMAN.NAVTEXT[6] = "Er funktioniert nicht mehr, das Wasser ist eingefroren. GlÃ¼cklicherweise kenne ich andere Wege meinen Wasserhaushalt zu regulieren. Aber das Blut hier...kommt mir bekannt vor.";
KrinLang.GERMAN.NAVTITLE[7] = "Das Auto";
KrinLang.GERMAN.NAVTEXT[7] = "Das Auto mit dem Felicity versucht hat zu entkommen. Veradux hat es mit einem Elektro-Blitz lahm gelegt. Die Battery scheint kaputt zu sein. Im Auto war ein Kassettenspieler. Ich kann es immer noch nicht glauben... wie soll dieses Lied mir helfen? Vielleicht hat Louis mir die falsche Kassette gegeben. Behalten sollte ich es trotzdem; Felicity dachte offensichtlich dass es wichtig ist.";
KrinLang.GERMAN.NAVTITLE[8] = "Der Mond";
KrinLang.GERMAN.NAVTEXT[8] = "Heute ist der Mond so gross und rot. Irgendwas stimmt nicht. Es scheint als ob die Nacht ewig anhalten wÃ¼rde.";
KrinLang.GERMAN.NAVTITLE[9] = "Die Kathedrale";
KrinLang.GERMAN.NAVTEXT[9] = "Ein weit entferntes GebÃ¤ude. Es ist zu weit um dort hin zu laufen, aber eine Terrasse ist in der NÃ¤he.";
KrinLang.GERMAN.NAVTITLE[10] = "Ed Spencer";
KrinLang.GERMAN.NAVTEXT[10] = "Guten Abend, Fremder. Ich warte schon lange hier auf jemanden der die Bahn fahren kann. Ich wÃ¼rde nicht in die anderen Abteile gehen. Die Leute aus der Filmindustrie waren dort am feiern...bis er auftauchte.";
KrinLang.GERMAN.NAVTITLE[11] = "Zeitungsausschnitt";
KrinLang.GERMAN.NAVTEXT[11] = "Es gibt Bedenken Ã¼ber den brÃ¼hmten, verschwundenen Magier, Clemons Bugiel. Er ist seit drei Wochen verschollen, und bie Polizei hat noch keine Spuren. Bugiel ist ein Magier, der fÃ¼r seine Tricks; VerhÃ¼llungen, Illusionen, Hypnosis weltbekannt wurde. Zuletzt wurde er im \'The Floating Apple\' gesehen, bevor er ohne jeglich Spur verschwand. Ist dies einer seiner Tricks, oder ist es echt? Die Polizei fÃ¼rchtet das Schlimmste.";
KrinLang.GERMAN.NAVTITLE[12] = "Offenes Abteil";
KrinLang.GERMAN.NAVTEXT[12] = "Ein StÃ¼ck Papier liegt am Boden.Namen und Nummern sind auf der Liste. Die meisten sind durch gekreuzt.";
KrinLang.GERMAN.NAVTITLE[13] = "Das MenÃ¼";
KrinLang.GERMAN.NAVTEXT[13] = "Es gibt Tomatensuppe, Spaghetti... und der Rest ist mit Blut beschmiert. Aber es riecht komisch, nach Chlor.";
